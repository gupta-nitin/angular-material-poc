SELECT hcs.busCheckGroup, hcs.severity, ccv.dqGroupCheckDescription, ccv.versionNum, ccv.validFrom, SUM(hcs.totalRecordCount), SUM(hcs.recordCount) FROM HealthCheckSummary hcs JOIN ConditionalChecksVersioned ccv ON hcs.busCheckGroup = ccv.busCheckGroup AND hcs.ruleVerNum = ccv.versionNum WHERE hcs.execId = "---" GROUP BY hcs.busCheckGroup, hcs.severity, ccv.validFrom, ccv.dqGroupCheckDescription, ccv.versionNum
